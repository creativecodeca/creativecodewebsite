# Gemini API Key (for AI website generation)
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub Personal Access Token (for creating repositories)
# Create at: https://github.com/settings/tokens
# Required scopes: repo (full control of private repositories)
GITHUB_TOKEN=your_github_personal_access_token_here

# Vercel API Token (for auto-deploying generated websites)
# Create at: https://vercel.com/account/tokens
# Required for automatic Vercel deployment
VERCEL_TOKEN=your_vercel_api_token_here

# GoHighLevel API (existing)
GHL_API_KEY=your_ghl_api_key_here
GHL_LOCATION_ID=your_ghl_location_id_here

# Server Configuration
PORT=4000
FRONTEND_ORIGIN=https://creativecodeca.com
